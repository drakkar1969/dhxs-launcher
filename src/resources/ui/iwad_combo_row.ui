<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!--// IWadComboRow template //-->
  <template class="IWadComboRow" parent="AdwComboRow">
    <property name="model">
      <object class="GtkSortListModel" id="sort_model">
        <property name="model">
          <object class="GListStore" id="model">
            <property name="item-type">IWadObject</property>
          </object>
        </property>
        <property name="sorter">
          <object class="GtkStringSorter" id="sorter">
            <property name="expression">
              <lookup name="name" type="IWadObject"/>
            </property>
          </object>
        </property>
      </object>
    </property>
    <property name="factory">
      <object class="GtkBuilderListItemFactory">
        <property name="resource">/com/github/D-Launcher/ui/iwad_combo/entry-item.ui</property>
      </object>
    </property>
    <property name="list-factory">
      <object class="GtkBuilderListItemFactory">
        <property name="resource">/com/github/D-Launcher/ui/iwad_combo/list-item.ui</property>
      </object>
    </property>
  </template>
</interface>
