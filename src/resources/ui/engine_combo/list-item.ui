<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GtkListItem">
    <property name="child">
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox">
            <property name="spacing">24</property>
            <child>
              <object class="GtkLabel">
                <binding name="label">
                  <lookup name="name" type="EngineObject">
                    <lookup name="item">GtkListItem</lookup>
                  </lookup>
                </binding>
                <property name="xalign">0</property>
                <style>
                  <class name="heading"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkLevelBar">
                <property name="hexpand">true</property>
                <property name="halign">end</property>
                <property name="valign">center</property>
                <property name="mode">discrete</property>
                <property name="min-value">0</property>
                <property name="max-value">5</property>
                <binding name="value">
                  <lookup name="compatibility" type="EngineObject">
                    <lookup name="item">GtkListItem</lookup>
                  </lookup>
                </binding>
                <property name="tooltip-text">Vanilla Compatibility</property>
                <offsets>
                  <offset name="low" value="0"/>
                  <offset name="high" value="5"/>
                  <offset name="full" value="5"/>
                </offsets>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkLabel">
            <binding name="label">
              <lookup name="description" type="EngineObject">
                <lookup name="item">GtkListItem</lookup>
              </lookup>
            </binding>
            <property name="xalign">0</property>
            <property name="wrap">true</property>
            <property name="max-width-chars">60</property>
            <style>
              <class name="caption"/>
              <class name="dimmed"/>
            </style>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
